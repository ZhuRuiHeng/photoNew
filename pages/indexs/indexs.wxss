/**index.wxss**/
@import "../template/template.wxss";
page{
  background-color: #e4e4e4;
}
.container{
  font-size: 26rpx;
  overflow: hidden;
  background-color: #e4e4e4;
  margin-bottom: 100rpx;
}
.management{
    display: flex;
    justify-content: space-between;
    height: 70rpx;
    line-height: 70rpx;
    padding: 10rpx 20rpx;
    margin-top: 30rpx;
}
.management .left{
  width: 70rpx;
  height: 70rpx;
  z-index: 1;
  border-radius: 50%;
  background: url(https://friend-check.playonwechat.com/static/images/stop_music.png) no-repeat;
  background-size: 100%;
}
.left.music_play{
  background: url(https://friend-check.playonwechat.com/static/images/music.png) no-repeat;
  background-size: 100%;
  -webkit-animation: music 3s linear infinite;
  animation: music 3s linear infinite;
  -webkit-tap-highlight-color: transparent;
}
@-webkit-keyframes music{
    0%{
        -webkit-transform:rotate(0);
        transform:rotate(0)
    }
    to{
        -webkit-transform:rotate(-3turn);
        transform:rotate(-3turn)
    }
}
@keyframes music{
    0%{
        -webkit-transform:rotate(0);
        transform:rotate(0)
    }
    to{
        -webkit-transform:rotate(-3turn);
        transform:rotate(-3turn)
    }
}

.management .right{
    background: #34a8e0;
    color: #fff;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 10rpx;
    padding: 0 20rpx;
}
.notice{
  text-align: center;
}
.buttons view{
  display: block;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  padding: 0 20rpx;
  background: #666;
  color: #fff;
  margin: 20rpx auto;
  text-align: center;
  cursor: pointer;
}
.buttons view.active{
  background: #34a8e0;
}
.buttons button{
  font-size: 26rpx;
  margin: 0;
  padding: 0;
  border-radius: 0;
  background: #34a8e0;
  color: #fff;
  line-height: inherit;
}
button::after{
  content: none;
}
.photo{
  position: relative;
  width: 710rpx;
  margin: 50rpx auto;
  height: 640rpx;
  /* background:#fff; */
  background: url(https://gcdn.playonwechat.com/photo/bg.png) no-repeat;
  background-size: contain; 
}
.photo image{
  /* border:1px solid #ddd; */
   position:absolute;
    opacity:1;
    -webkit-transform:scale(2);
    -ms-transform:scale(2);
    transform:scale(2);
    transition:opacity 2s,-webkit-transform 2s;
    transition:opacity 2s,transform 2s;
    transition:opacity 2s,transform 2s,-webkit-transform 2s;
    /* border:1px solid #fff;  */
    box-sizing: border-box;
    background: pink;
}
 .photo image.ball{
    opacity:1;
    -webkit-transform:scale(1);
    -ms-transform:scale(1);
    transform:scale(1);
} 
.photo image{
  width:66rpx;
    height:66rpx;
}
.photo image.item1,.photo image.item2,.photo image.item3,.photo image.item4{
    width:170rpx;
    height:170rpx;
}

@-webkit-keyframes beat{
    0%{
        -webkit-transform:scale(1);
        transform:scale(1)
    }
    5%{
        -webkit-transform:scale(1.04);
        transform:scale(1.04)
    }
    10%{
        -webkit-transform:scale(1.02);
        transform:scale(1.02)
    }
    15%{
        -webkit-transform:scale(1.04);
        transform:scale(1.04)
    }
    50%{
        -webkit-transform:scale(1);
        transform:scale(1)
    }
    to{
        -webkit-transform:scale(1);
        transform:scale(1)
    }
}

page{
  background: #e4e4e4;
}
.container{
    overflow: hidden;
    padding:20rpx 10rpx;
    box-sizing: border-box;
    width: 100%;
    font-size: 26rpx;
    background-color: #e4e4e4;
    margin-bottom: 100rpx;
}
.item{
    width: 31%;
    position: relative;
    padding: 10rpx;
    margin: 1%;
    background: #fff;
    box-sizing: border-box;
    display: inline-block;
}
.item image{
  width: 100%;
  height: 180rpx;
}
.timeWho input{
   border-top: 1px solid #ddd;
    width: 100%;
    text-decoration:none; 
    display:block; 
    float:left;  
    overflow:hidden;          /*超出部分隐藏*/
    white-space:nowrap;
    /*文本溢出用"..."表示*/
    text-overflow:ellipsis;                    /*适用IE*/
    -o-text-overflow:ellipsis;                 /*适用opera*/
    -moz-binding: url('ellipsis.xml#ellipsis');/*适用火狐(firefox通过XUL实现效果)*/
}
.newAdd{
  position: relative
}
.musicTip{
  width: 100%;
  display: flex;
  justify-content: flex-start;
}
.musicTip image{
    width: 60rpx;
    height: 60rpx;
}
.musicTip view{
  width:100%;
  text-decoration:none; 
    display:block; 
    float:left;  
    overflow:hidden;          /*超出部分隐藏*/
    white-space:nowrap;
    /*文本溢出用"..."表示*/
    text-overflow:ellipsis;                    /*适用IE*/
    -o-text-overflow:ellipsis;                 /*适用opera*/
    -moz-binding: url('ellipsis.xml#ellipsis');/*适用火狐(firefox通过XUL实现效果)*/
}
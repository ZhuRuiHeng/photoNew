<template name="navBar">
    <view class='navBarbox'>
        <view class="navBar">
            <view class='itemBar' data-itemBar='1' bindtap='navUrl' data-url='../square/square'>
                <image wx:if="{{itemBar==1}}" src="../img/home-icon-active.png" mode='widthFix'></image>
                <image wx:else src="../img/home-icon1.png" mode='widthFix'></image>
            </view>
            <view class='itemBar' data-itemBar='2' bindtap='navUrl' data-url='../photos/photos'>
                
                <image wx:if="{{itemBar==2}}" src="../img/photo1-hover.png"  mode='widthFix'></image>
                <image wx:else src="../img/photo1.png"  mode='widthFix'></image>
            </view>
            <view class='itemBar itemBar3' data-itemBar='3' bindtap='navUrl' data-url='../my/my'>
                <image wx:if="{{itemBar==3}}" src="../img/my_hover.png"  mode='widthFix'></image>
                <image wx:else src="../img/my.png"  mode='widthFix'></image>
            </view>
            <view class='itemBar' data-itemBar='41' bindtap='navUrl' data-url='../more/more'>
                <image wx:if="{{itemBar==4}}" src="../img/more_hover.png"  mode='widthFix'></image>
                <image wx:else src="../img/more.png"  mode='widthFix'></image>
            </view>
        </view>
    </view>
</template>
<template name="pretty">
    <view class='prettyBox'>
        <view class='pretty triangle'>
            <navigator url="../templatePhoto/templatePhoto"   class="pretty1" >
                <image src="../img/1422830-200.png" ></image><text>照片墙</text>
            </navigator>
            <navigator url="../album/album" class="pretty2"  >
                <image src="../img/1419919-200.png" ></image><text>相册集</text>
            </navigator>
        </view>
    </view>
</template>